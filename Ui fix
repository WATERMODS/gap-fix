getgenv().SecureMode = true

local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()

local Window = Rayfield:CreateWindow({

	Name = "FIX MENU",	LoadingTitle = "FIX MENU",

	LoadingSubtitle = "by WATER X",

	ConfigurationSaving = {

		Enabled = true,

		FolderName = nil, -- Create a custom folder for your hub/game

		FileName = "Big Hub"

	},

        Discord = {

        	Enabled = false,

        	Invite = "siriulllllls", -- The Discord invite code, do not include discord.gg/

        	RememberJoins = true -- Set this to false to make them join the discord every time they load it up

        },

	KeySystem = FALSE, -- Set this to true to use our key system

	KeySettings = {

		Title = "FIXb",

		Subtitle = "Key System",

		Note = "Join the discord (discord.gg/)",

		FileName = "SiriusKey",

		SaveKey = true,

		GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from

		Key = "Hello"

	}

})

local Tab = Window:CreateTab("Spawn gui hub", 448336288)

local spawn hubButton = spawn gui hubtab:CreateButton({

	Name = "Button Example",

	Callback = function()

		loadstring(game:HttpGet("https://pastebin.com/raw/kPrr9y0M"))()

	end,

})
